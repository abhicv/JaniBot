<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find jani_arm_control)/config/ros_controllers.yaml" command="load"/>  

  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="arm_controller camera_yaw_controller camera_pitch_controller gripper_controller joint_state_controller wheel_joint_back_left_controller wheel_joint_back_right_controller wheel_joint_front_left_controller wheel_joint_front_right_controller"/>

</launch>
