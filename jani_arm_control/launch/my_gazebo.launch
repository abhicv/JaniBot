<?xml version="1.0"?>
<launch>
  <arg name="default_map" default="false"/>
  <arg name="world" default="config_1.world"/>
  <arg name="gui" default="true" />
  <param name="robot_description" command="$(find xacro)/xacro $(find robot_description)/urdf/robot.xacro"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find jani_world)/worlds/$(arg world)" unless="$(arg default_map)"/>
    <arg name="world_name" value="worlds/empty.world" if="$(arg default_map)"/>
    <arg name="paused" value="false"/>
    <arg name="gui" value="$(arg gui)"/>
  </include>
  <node name="spawn_robot" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot -x -0.4 -y 0 -z 0"  
    respawn="false" output="screen" />
  <!--  <node name="spawn_cup" pkg="gazebo_ros" type="spawn_model" args="-sdf -file $(find jani_world)/models/cup/model.sdf -model cup -x 0.585309 -y -0.367232 -z 0"  
    respawn="false" output="screen" />-->
     <!--<node name="spawn_cup" pkg="gazebo_ros" type="spawn_model" args="-sdf -file $(find jani_world)/models/cup/model.sdf -model cup -x 0.137935  -y -0.934268 -z 0"  
    respawn="false" output="screen" />-->
        <node name="spawn_coke" pkg="gazebo_ros" type="spawn_model" args="-sdf -file $(find jani_world)/models/coke_can/model.sdf -model coke -x 0.566508 -y  -0.208797 -z 0"  
    respawn="false" output="screen" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="true" output="screen" />   
  <include file="$(find jani_arm_control)/launch/ros_controllers.launch"/> 
</launch>
