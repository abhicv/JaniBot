<launch>

<rosparam file="$(find arm_export_description)/launch/controller.yaml" command="load"/>
<node args="motor_1_position_controller motor_2_position_controller motor_4_position_controller motor_5_position_controller motor_3_position_controller joint_state_controller " name="controller_spawner" ns="arm_export" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/arm_export/joint_states"/>
</node>

</launch>