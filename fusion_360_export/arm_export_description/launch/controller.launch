<launch>

<rosparam file="$(find arm_export_description)/launch/controller.yaml" command="load"/>
<node args="Rev32_position_controller Rev34_position_controller Rev36_position_controller Rev39_position_controller Rev40_position_controller joint_state_controller " name="controller_spawner" ns="arm_export" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/arm_export/joint_states"/>
</node>

</launch>